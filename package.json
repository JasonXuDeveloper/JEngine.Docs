{
  "name": "jengine.docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "bun run favicons && bun run optimize-images",
    "build": "next build",
    "build:deploy": "bun run prebuild && next build && bun run search:sync",
    "dev": "next dev",
    "start": "next start",
    "preview": "bun run prebuild && opennextjs-cloudflare build && bun run search:sync && opennextjs-cloudflare preview",
    "deploy": "bun run prebuild && opennextjs-cloudflare build && bun run search:sync && opennextjs-cloudflare deploy && bun run indexnow",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "lint:links": "bun run ./scripts/lint-links.ts",
    "search:sync": "bun run ./scripts/sync-algolia.ts",
    "favicons": "bun run ./scripts/generate-favicons.ts",
    "optimize-images": "bun run ./scripts/optimize-images.ts",
    "indexnow": "bun run ./scripts/submit-indexnow.ts",
    "format": "biome format --write"
  },
  "dependencies": {
    "@octokit/auth-app": "^8.2.0",
    "@opennextjs/cloudflare": "^1.16.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-popover": "^1.1.15",
    "algoliasearch": "^5.47.0",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "16.5.0",
    "fumadocs-mdx": "14.2.6",
    "fumadocs-ui": "16.5.0",
    "lucide-react": "^0.563.0",
    "next": "16",
    "next-validate-link": "^1.6.4",
    "octokit": "^5.0.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "sharp-ico": "^0.1.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "wrangler": "^4.63.0"
  }
}